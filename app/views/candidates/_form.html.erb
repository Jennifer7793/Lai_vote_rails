<%= form_for (candidate) do |f| %>

  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  
  <div>
    <%= f.label :age %>
    <%= f.text_field :age %>
  </div>
  
  <div>
    <%= f.label :party %>
    <%= f.text_field :party %>
  </div>
  
  <div>
    <%= f.label :politics %>
    <%= f.text_field :politics %>
  </div>
    <%= f.submit 'Edited' %>
<% end %>